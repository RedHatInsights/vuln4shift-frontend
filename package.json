{
  "name": "ocp-vulnerability-frontend",
  "version": "1.1.0",
  "private": false,
  "engines": {
    "node": ">=22.11.0",
    "npm": ">=10.0.0"
  },
  "scripts": {
    "build": "fec build",
    "lint": "npm-run-all lint:*",
    "lint:js": "eslint config src",
    "lint:js:fix": "eslint config src --fix",
    "lint:sass": "stylelint 'src/**/*.scss' --config .stylelintrc.json",
    "patch:hosts": "fec patch-etc-hosts",
    "start": "fec dev",
    "start:proxy": "PROXY=true fec dev",
    "test": "npm run test:ct",
    "test:coverage": "curl -sSL 'https://raw.githubusercontent.com/RedHatInsights/insights-interact-tools/refs/heads/main/scripts/coverage.sh' | bash",
    "test:ct": "BABEL_ENV=componentTest cypress run --component",
    "ci:verify": "npm run test:coverage",
    "verify": "npm-run-all build lint test",
    "postinstall": "ts-patch install"
  },
  "dependencies": {
    "@patternfly/react-component-groups": "^5.5.8",
    "@patternfly/react-core": "^5.4.14",
    "@patternfly/react-icons": "^5.4.2",
    "@patternfly/react-table": "^5.4.16",
    "@redhat-cloud-services/frontend-components": "^7.0.9",
    "@redhat-cloud-services/frontend-components-notifications": "^6.1.11",
    "@redhat-cloud-services/frontend-components-utilities": "^7.0.8",
    "@sentry/webpack-plugin": "^3.5.0",
    "@tanstack/react-query": "^5.82.0",
    "@unleash/proxy-client-react": "^5.0.0",
    "axios": "^1.10.0",
    "declarative-table": "^5.0.9",
    "lodash": "^4.17.21",
    "prop-types": "^15.8.1",
    "query-string": "^9.2.2",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-redux": "^9.2.0",
    "react-router-dom": "^6.30.1",
    "redux": "^5.0.1",
    "redux-promise-middleware": "^6.2.0",
    "use-deep-compare-effect": "^1.8.1"
  },
  "devDependencies": {
    "@babel/core": "^7.28.0",
    "@babel/plugin-transform-runtime": "^7.28.0",
    "@babel/preset-env": "^7.28.0",
    "@babel/preset-react": "^7.27.1",
    "@babel/runtime": "^7.27.6",
    "@commitlint/cli": "^19.8.1",
    "@commitlint/config-conventional": "^19.8.1",
    "@cypress/code-coverage": "^3.14.5",
    "@cypress/react18": "^2.0.1",
    "@cypress/webpack-dev-server": "^3.11.0",
    "@frsource/cypress-plugin-visual-regression-diff": "^3.3.1",
    "@patternfly/patternfly": "^5.4.2",
    "@redhat-cloud-services/eslint-config-redhat-cloud-services": "^3.0.0",
    "@redhat-cloud-services/frontend-components-config": "^6.7.1",
    "@redhat-cloud-services/tsc-transform-imports": "^1.0.25",
    "babel-plugin-istanbul": "7.0.0",
    "babel-plugin-transform-imports": "^2.0.0",
    "cypress": "^13.17.0",
    "eslint": "^8.57.1",
    "eslint-plugin-cypress": "^2.15.2",
    "eslint-plugin-no-only-tests": "^3.3.0",
    "npm-run-all": "4.1.5",
    "strict-uri-encode": "^2.0.0",
    "stylelint": "^16.21.1",
    "stylelint-config-recommended-scss": "^15.0.1",
    "ts-patch": "^3.3.0",
    "typescript": "^5.8.3",
    "webpack": "^5.99.9",
    "webpack-bundle-analyzer": "^4.10.2"
  },
  "insights": {
    "appname": "ocp-vulnerability"
  },
  "nyc": {
    "report-dir": "coverage-cypress",
    "include": [
      "src/**/*.{js,jsx}"
    ],
    "exclude": [
      "src/**/*.spec.ct.js",
      "src/**/*.test.js",
      "src/**/*.cy.js"
    ],
    "all": true
  }
}
